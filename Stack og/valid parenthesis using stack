import java.util.*;
public class ValidParenthesis{
    public static boolean checkparenthesis(String s1){
        Stack<Character>stack=new Stack<>();
        for(char ch:s1.toCharArray()){
            if(ch=='{' || ch=='[' || ch=='('){
                stack.push(ch);
            }
            else{
                if(stack.isEmpty()){
                    return false;
                }
                char top=stack.pop();
                if((ch=='}' && top !='{')||
                    (ch==']' && top !='[') ||
                     (ch==')' && top !='(')){
                         return false;
                     }
               
            }
       
        }
              return stack.isEmpty();
    }
    public static void main(String args[]){
        String s1="[{{}]";
        System.out.println(checkparenthesis(s1));
        
    }
}
