class Solution {
    public boolean isValid(String s) {
   
        String regex = "^("
                + "(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])\\.){3}"
                + "(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])$";
        
        
        if(s.matches(regex)){
            return true;
        }
       return false; 
    }
}
